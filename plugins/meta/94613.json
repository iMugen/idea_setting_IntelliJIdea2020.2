ation"],"vendor":"Yii.Guxing","version":"2.9.2","notes":"\n      <ul>\n        <li>添加快速切换自动文档翻译的快捷键</li>\n        <li>修复了一些Bug</li>\n      </ul>\n      <a href=\"https://github.com/YiiGuxing/TranslationPlugin/blob/master/CHANGELOG.md\"><b>Full Changelog History</b></a>\n    ","optionalDependencies":["com.intellij.modules.lang","com.intellij.modules.cidr.lang","org.jetbrains.kotlin","org.jetbrains.plugins.go","com.intellij.modules.python","Dart","com.intellij.css","org.intellij.groovy","com.intellij.modules.java","JavaScript","com.intellij.modules.json","com.intellij.jsp","com.jetbrains.php","com.intellij.properties","com.jetbrains.restClient","com.intellij.modules.ruby","org.intellij.scala","com.jetbrains.sh","com.intellij.modules.sql","com.intellij.modules.swift.lang","org.jetbrains.plugins.vue","com.intellij.modules.xml","XPathView","org.jetbrains.plugins.yaml"],"since":"193.0","sourceCodeUrl":"https://github.com/YiiGuxing/TranslationPlugin","size":2252401,"url":"http://yiiguxing.github.io/TranslationPlugin"}